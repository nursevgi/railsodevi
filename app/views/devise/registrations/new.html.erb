<h2><%= t"Sign_up" %></h2>
<div class="panel panel-default widget">
<div class="panel-body">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="form-group">
    <%= f.text_field :name,class:"form-control",placeholder: t('Name')%>
  </div>

  <div class="form-group">
    <%= f.email_field :email, autofocus: true,class:"form-control",placeholder: t('Email') %>
  </div>

  <div class="form-group">
    <%= f.password_field :password, autocomplete: "off",class:"form-control",placeholder: t('Password_new') %>
  </div>

  <div class="form-group">
    <%= f.password_field :password_confirmation, autocomplete: "off",class:"form-control",placeholder: t('Password_again') %>
  </div>

  <div class="form-group">
    <%= f.file_field :image,class:"form-control",placeholder:"Bir Profil Resmi" %>
  </div>
  <div class="form-group">
    <%= f.submit t('Sign_up'),class:"form-control btn btn-primary",placeholder:"Ä°sminiz" %>
  </div>
<% end %>
</div>
</div>
<%= render "devise/shared/links" %>
